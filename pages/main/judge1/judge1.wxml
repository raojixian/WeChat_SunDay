<!--pages/main/judge1/judge1.wxml-->
<view class="page">
  <view class="item" wx:for="{{order}}" wx:for-item="key" id = "{{key}}">
    <text class="item_left" id = "{{key}}">{{items[key][1]}}</text>
    <view class="item_right" id = "{{key}}" bindtap="itemtap" >{{show[key]}}</view>
  </view>
  <button bindtap="submit" style="margin-top: 40rpx;">批改</button>
</view>
